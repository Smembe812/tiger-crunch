    "level": "^7.0.0",
    "node-forge": "^0.10.0",
    "nodemailer": "^6.6.0",
    "openid-client": "^4.4.0",
    "qrcode": "^1.4.4",
    "speakeasy": "^2.0.0",
    "validator": "^13.6.0",
    "winston": "^3.3.3",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.27",
    "@types/sinon": "^9.0.10",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "eslint": "^7.19.0",
