        "cookie-parser": "^1.4.5",
        "cookie-session": "^1.4.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-fingerprint": "^1.2.2",
        "geoip-lite": "^1.4.2",
        "googleapis": "^73.0.0",
        "graphql": "^15.5.0",
        "helmet": "^4.4.1",
        "jsonwebtoken": "^8.5.1",
        "level": "^7.0.0",
