        "chai-as-promised": "^7.1.1",
        "chai-subset": "^1.6.0",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "lerna": "^4.0.0",
        "mocha": "^8.2.1",
        "nodemon": "^2.0.4",
        "proxyquire": "^2.1.3",
